{
  "name": "scaips-backend",
  "version": "1.0.0",
  "description": "Backend API for Student College Alumni Industry Professional Startup (SCAIPS) platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "setup-render": "node scripts/setupRenderDB.js",
    "test-db": "node scripts/testDatabase.js",
    "troubleshoot": "node scripts/troubleshootDB.js",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed",
    "db:users": "node scripts/viewUsers.js",
    "db:adduser": "node scripts/addUser.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset"
  },
  "keywords": [
    "express",
    "postgresql",
    "sequelize",
    "jwt",
    "authentication",
    "alumni",
    "student",
    "startup"
  ],
  "author": "SCAIPS Team",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/generative-ai": "^0.24.1",
    "@ilovepdf/ilovepdf-nodejs": "^0.3.1",
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "generative-ai": "^0.1.4",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^2.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.29.0",
    "redis": "^5.10.0",
    "sequelize": "^6.35.0",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "streamifier": "^0.1.1",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "jest": "^25.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^5.22.0",
    "supertest": "^7.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
